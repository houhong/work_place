package xom.it.houhong.二叉树;

import java.util.Comparator;

/*
 * 二叉树搜索树
 */

public class BST<E extends Comparable<E>>{

    //定义节点
	private class Node{
		
		
		public E e;
		public Node left,right;
		public Node(E e) {
			
			this.e = e;
			this.left = null;
			this.right = null;
		}
		
		
		
	}
	
	private Node root;
	private int size;
	
    


	public BST() {
		root = null;
		size = 0;
	}




	public Node getRoot() {
		return root;
	}




	public void setRoot(Node root) {
		this.root = root;
	}




	public int getSize() {
		return size;
	}




	public void setSize(int size) {
		this.size = size;
	}
	
