package it.input.demo1;

import java.io.File;



/*
 *  字节流复制文件
 *   采用数组缓冲提高效率
 *   字节数组
 *   FileInputStream 读取字节数组
 *   FileOutputStream 写字节数组
 */

public class input {

	public static void main(String[] args) {
		//给出现在的时间：
		/*long s = System.currentTimeMillis();
		//System.out.println(s);*/		
		File file = new File("f:\\copy.txt");
		file.createNewFile();
		file.mkdir();
	}


}
